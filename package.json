{
  "name": "example-electron-app",
  "version": "0.0.0",
  "description": "",
  "engines": {
    "node": ">=8.9.0",
    "npm": ">=5.5.1"
  },
  "main": "src/__bootstrap/index.js",
  "scripts": {
    "start": "npm run start-ui &> /dev/null & sleep 5; electron .",
    "start-ui": "cd src/ui/electron/view; npm start",
    "build-ui": "cd src/ui/electron/view; npm run build",
    "test": "eslint ./src && jest",
    "postinstall": "npm run prestart-ui",
    "prepack": "npm test",
    "prestart-ui": "cd src/ui/electron/view; npm i --no-progress; rm -rf build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "esm": "^3.0.14",
    "node-fetch": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.0",
    "electron-packager": "^14.0.0",
    "eslint": "^6.0.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-react": "^7.0.0",
    "jest": "^23.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/src/__bootstrap/polyfills.js"
    ]
  }
